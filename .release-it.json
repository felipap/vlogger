{
  "git": {
    "commitMessage": "chore(release): v${version}",
    "tagName": "v${version}",
    "requireCleanWorkingDir": true
  },
  "github": {
    "release": true,
    "releaseName": "Vlogger ${version}"
  },
  "npm": {
    "publish": false
  },
  "hooks": {
    "after:bump": "echo 'Version bumped to ${version}'"
  }
}
